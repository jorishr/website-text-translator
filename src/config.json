{
  "languages": {
    "base": "nl",
    "targets": ["en", "es", "ca"]
  },
  "mode": {
    "backup": true,
    "noTranslate": false,
    "dryRun": false,
    "logLevel": "verbose"
  },
  "id": {
    "txtId": "data-txt_id",
    "altId": "data-txt_id__alt",
    "titleId": "data-txt_id__title",
    "plchldrId": "data-txt_id__placeholder",
    "metaId": "data-txt_id__meta"
  },
  "offset": 100,
  "elements": {
    "selectors": "head meta[name=description], head meta[name=keywords], head title, h1, h2, h3, h4, h5, h6, p, span, a, img, strong, em, input",
    "exclude": {
      "classesToExclude": [],
      "idsToExclude": []
    }
  },
  "folders": {
    "src": "./",
    "dest": "./",
    "backup": "./backup"
  },
  "fileNames": {
    "prefix": "txt_data_"
  },
  "direction": {
    "direction": "",
    "default": "json2html",
    "jsonToHtml": "json2html",
    "htmlToJson": "html2json"
  }
}
